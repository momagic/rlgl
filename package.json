    "alchemy-sdk": "^3.6.5",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "eruda": "^3.4.3",
    "ethers": "^6.15.0",
    "express": "^4.21.2",
    "i18next": "^25.6.3",
    "i18next-browser-languagedetector": "^8.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.7.4",
    "tailwind-merge": "^3.4.0",
    "viem": "^2.39.3"
