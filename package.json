{
  "name": "rlgl",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"pnpm run dev:api\" \"pnpm run dev:client\"",
    "dev:api": "node dev-server.js",
    "dev:client": "vite",
    "build": "tsc -b && vite build && node scripts/build-with-eruda.cjs",
    "lint": "eslint .",
    "preview": "vite preview",
    "compile": "npx hardhat compile",
    "deploy:worldchain": "npx hardhat run scripts/deploy.cjs --network worldchain",
    "deploy:sepolia": "npx hardhat run scripts/deploy.cjs --network worldchain-sepolia",
    "deploy:mainnet": "npx hardhat run scripts/deploy.cjs --network worldchain",
    "deploy-v2:worldchain": "npx hardhat run scripts/deploy-v2.cjs --network worldchain",
    "deploy-v2:sepolia": "npx hardhat run scripts/deploy-v2.cjs --network worldchain-sepolia",
    "deploy-v2:mainnet": "npx hardhat run scripts/deploy-v2.cjs --network worldchain",
    "deploy-v3:worldchain": "npx hardhat run scripts/deploy-v3.cjs --network worldchain",
    "deploy-v3:sepolia": "npx hardhat run scripts/deploy-v3.cjs --network worldchain-sepolia",
    "deploy-v3:mainnet": "npx hardhat run scripts/deploy-v3.cjs --network worldchain",
    "seed-leaderboard:worldchain": "npx hardhat run scripts/seed-leaderboard.cjs --network worldchain",
    "seed-leaderboard:sepolia": "npx hardhat run scripts/seed-leaderboard.cjs --network worldchain-sepolia",
    "withdraw:mainnet": "npx hardhat run scripts/withdraw.cjs --network worldchain",
    "check-earnings": "npx hardhat run scripts/check-earnings.cjs --network worldchain",
    "update-price": "npx hardhat run scripts/update-price.cjs --network worldchain",
    "migrate-to-v3": "node scripts/migrate-to-v3.cjs",
    "test:v3": "node scripts/test-v3-contract.cjs",
    "test:v3:quick": "npx hardhat test test/RedLightGreenLightGameV3.test.cjs",
    "verify:worldchain": "npx hardhat verify --network worldchain",
    "verify:sepolia": "npx hardhat verify --network worldchain-sepolia"
  },
  "dependencies": {
    "@fontsource/jetbrains-mono": "^5.2.8",
    "@fontsource/noto-sans-kr": "^5.2.8",
    "@fontsource/orbitron": "^5.2.8",
    "@fontsource/rajdhani": "^5.2.7",
    "@upstash/redis": "^1.35.6",
