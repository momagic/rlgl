    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.22",
    "chai-as-promised": "^8.0.2",
    "dotenv": "^16.6.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.24",
    "hardhat": "^2.27.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0",
    "vite": "^7.2.4"
  }
