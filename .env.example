# Environment Variables for Red Light Green Light Game
# Copy this file to .env and fill in your actual values

# Your wallet private key (required for contract interactions)
# Get this from your wallet (MetaMask, etc.) - NEVER share this!
PRIVATE_KEY=your_private_key_here

# Optional: World Chain Explorer API key for contract verification
WORLDSCAN_API_KEY=your_worldscan_api_key_here

# Example of what your private key should look like (64 characters):
# PRIVATE_KEY=0x1234567890abcdef1234567890abcdef1234567890abcdef1234567890abcdef

# Frontend World ID integration
# Base URL for the World ID Verification API (include /api if proxied)
VITE_WORLD_ID_API_URL=https://your-domain.com/api
# Base URL for secure score permit API used by client
VITE_VERIFICATION_API_BASE=https://your-domain.com/api
# Your World ID App ID (from Worldcoin)
VITE_WORLD_ID_APP_ID=your_world_id_app_id_here
# Action ID used in verification payload
VITE_WORLD_ID_ACTION_ID=play-game
VITE_LEADERBOARD_RPC_URL=

WORLD_ID_APP_ID=your_world_id_app_id_here
WORLD_ID_ACTION_ID=play-game
AUTHORIZED_SUBMITTER_PRIVATE_KEY=your_authorized_submitter_private_key_here
GAME_CONTRACT_ADDRESS=0xYourGameContractAddress
RPC_URL=https://worldchain-mainnet.g.alchemy.com/public
PORT=3000
NODE_ENV=production
CACHE_TTL_MINUTES=5